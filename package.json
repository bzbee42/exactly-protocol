{
  "name": "@exactly/protocol",
  "version": "0.2.10",
  "license": "BUSL-1.1",
  "homepage": "https://exact.ly",
  "repository": "github:exactly/protocol",
  "scripts": {
    "lint": "run-s --continue-on-error lint:**",
    "lint:sol": "solhint '{contracts,test}/**/*.sol'",
    "lint:ts": "tsc",
    "lint:es": "eslint --ext .ts,.js,.cjs,.mjs .",
    "fix:sol": "prettier -w '{contracts,test}/**/*.sol'",
    "compile": "hardhat compile",
    "snapshot": "forge snapshot --no-match-test '^(testFuzz|invariant)'",
    "test": "run-s --continue-on-error test:**",
    "test:foundry": "forge test",
    "test:hardhat": "hardhat test --deploy-fixture",
    "coverage": "run-s coverage:**",
    "coverage:foundry:snapshot": "forge snapshot --no-match-test '^(testFuzz|invariant)' --check",
    "coverage:hardhat": "run-s test:hardhat",
    "coverage:foundry": "FOUNDRY_PROFILE=production forge coverage --report lcov",
    "deploy:mainnet": "hardhat --network mainnet deploy",
    "deploy:goerli": "hardhat --network goerli deploy",
    "postinstall": "run-s --continue-on-error postinstall:** || cd .",
    "postinstall:foundry": "forge install",
    "postinstall:hardhat": "hardhat compile --force"
  },
  "files": [
    "contracts",
    "deployments",
    "hardhat.config.ts"
  ],
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "@openzeppelin/contracts": "4.9.0",
    "@openzeppelin/contracts-upgradeable": "4.9.0",
    "solmate": "transmissions11/solmate#v7"
  },
  "devDependencies": {
    "@changesets/cli": "^2.26.1",
    "@changesets/types": "^5.2.1",
    "@ethersproject/abi": "5.7.0",
    "@nomicfoundation/hardhat-chai-matchers": "^1.0.6",
    "@nomiclabs/hardhat-ethers": "npm:hardhat-deploy-ethers@^0.3.0-beta.13",
    "@openzeppelin/hardhat-upgrades": "^1.26.0",
    "@openzeppelin/upgrades-core": "^1.26.2",
    "@primitivefi/hardhat-dodoc": "^0.2.3",
    "@safe-global/safe-core-sdk": "^3.3.3",
    "@safe-global/safe-core-sdk-types": "^2.0.0",
    "@safe-global/safe-ethers-lib": "^1.9.3",
    "@safe-global/safe-service-client": "^2.0.1",
    "@tenderly/hardhat-tenderly": "^1.7.4",
    "@typechain/ethers-v5": "^11.0.0",
    "@typechain/hardhat": "^7.0.0",
    "@types/chai": "^4.3.5",
    "@types/eslint": "^8.40.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "^20.2.3",
    "@typescript-eslint/eslint-plugin": "^5.59.7",
    "@typescript-eslint/parser": "^5.59.7",
    "chai": "^4.3.7",
    "dotenv": "^16.0.3",
    "eslint": "^8.41.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-chai-expect": "^3.0.0",
    "eslint-plugin-chai-friendly": "^0.7.2",
    "eslint-plugin-deprecation": "^1.4.1",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-mocha": "^10.1.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "ethers": "^5.7.2",
    "hardhat": "2.14.0",
    "hardhat-contract-sizer": "^2.9.0",
    "hardhat-deploy": "^0.11.29",
    "hardhat-gas-reporter": "^1.0.9",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.8",
    "prettier-plugin-solidity": "^1.1.3",
    "solhint": "^3.4.1",
    "solhint-plugin-prettier": "^0.0.5",
    "solidity-coverage": "^0.8.2",
    "ts-node": "^10.9.1",
    "typechain": "^8.2.0",
    "typescript": "^5.0.4"
  },
  "resolutions": {
    "@nomiclabs/hardhat-ethers": "npm:hardhat-deploy-ethers@^0.3.0-beta.13"
  }
}
